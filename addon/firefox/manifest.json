{
    "manifest_version": 2,
    "name": "Pixiv Infinite Scroll",
    "version": "1.4.3.5",
    "description": "__MSG_extensionDescription__",
    "default_locale": "en",
    "icons": {
        "48": "icons/pis48.png",
        "96": "icons/pis96.png",
        "128": "icons/pis128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.pixiv.net/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}